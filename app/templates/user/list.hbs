{{#link-to "user.edit" user.id class="list-group-item list-group-item-action flex-column align-items-start"}}
  <div class="d-flex w-100 justify-content-between">
    <h5 class="mb-1">{{model.email}}</h5>
    <small>{{#if model.updatedAt}}{{moment-format model.updatedAt "MMM Do, YYYY h:mm a"}}{{/if}}</small>
  </div>
  <div class="mb-1">{{model.givenName}} {{model.familyName}}</div>
  <small>{{model.id}}</small>
{{/link-to}}
