{
  "name": "leads-manage",
  "version": "0.1.0",
  "private": true,
  "description": "Lead automation management application.",
  "license": "UNLICENSED",
  "author": "Jacob Bare <jacob@limit0.io>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/limit-zero/leads-manage.git",
  "scripts": {
    "prestart": "$npm_package_docker_dev_install",
    "start": "$npm_package_docker_dev_up",
    "start:local": "ember s --proxy=http://localhost:8288 --port=4206",
    "stop": "$npm_package_docker_dev_down",
    "terminal": "docker exec -it leadsmanage_app_1 /bin/bash",
    "build": "ember build",
    "lint:run": "./node_modules/.bin/eslint ./*.js app config lib server tests",
    "test": "ember test"
  },
  "docker": {
    "dev": {
      "install": "docker-compose -p leadsmanage run --no-deps --entrypoint yarn app install",
      "up": "docker-compose -p leadsmanage up",
      "down": "docker-compose -p leadsmanage down"
    }
  },
  "devDependencies": {
    "bootswatch": "^4.0.0",
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-apollo-client": "^1.0.0",
    "ember-cli": "~3.0.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-moment-shim": "^3.6.0",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sass": "^7.1.7",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-concurrency": "^0.8.17",
    "ember-data": "~3.0.0",
    "ember-entypo": "^1.0.1",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^3.4.4",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.6.0",
    "ember-notify": "^5.2.1",
    "ember-power-calendar": "^0.7.2",
    "ember-power-select": "^2.0.0-beta.3",
    "ember-resolver": "^4.0.0",
    "ember-route-action-helper": "^2.0.6",
    "ember-simple-auth": "^1.6.0",
    "ember-source": "~3.0.0",
    "ember-truth-helpers": "^2.0.0",
    "ember-welcome-page": "^3.0.0",
    "ember-wormhole": "^0.5.4",
    "eslint-plugin-ember": "^5.0.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.1"
  }
}
